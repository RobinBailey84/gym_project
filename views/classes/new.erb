<h1>Add New Class</h1>

<form action="/gymclasses" method="post">

  <label for="name">Class Name:</label>
  <input type="text" name="name">

  <label for="class_date">Class Date:</label>
  <input type="text" name="class_date">

  <label for="class_time">Class Time:</label>
  <input type="FLOAT" name="class_time">

  <label for="capacity">Capacity:</label>
  <input type="INTEGER" name="capacity">

  <label for="peak_time">Peak Time:</label>
  <select name="peak_time">
    <option value="true">True</option>
    <option value="false">False</option>
  </select>

  <input type="submit" Value="Add Class">

</form>
